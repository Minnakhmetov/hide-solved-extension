{
    "name": "Hide Solved",
    "description": "Hides solved contests on codeforces.com",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["https://codeforces.com/contests", "https://codeforces.com/contests/page*"],
            "js": ["script.js"]
        }
    ]
}